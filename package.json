{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "run-s \"build:*\"",
        "build:css": "npm run generate:css -- --minify",
        "build:remix": "remix build",
        "dev": "run-p \"dev:*\"",
        "dev:css": "npm run generate:css -- --watch",
        "dev:remix": "remix dev",
        "generate:css": "npx tailwindcss -i ./css/tailwind.css -o ./app/tailwind.css",
        "start": "cross-env NODE_ENV=production netlify dev"
    },
    "dependencies": {
        "@contentful/rich-text-html-renderer": "^15.13.1",
        "@contentful/rich-text-react-renderer": "^15.12.1",
        "@contentful/rich-text-types": "^15.12.1",
        "@headlessui/react": "^1.7.2",
        "@netlify/functions": "^1.0.0",
        "@remix-run/netlify": "^1.16.0",
        "@remix-run/node": "^1.16.0",
        "@remix-run/react": "^1.16.0",
        "classnames": "^2.3.1",
        "cross-env": "^7.0.3",
        "focus-trap-react": "^9.0.2",
        "graphql": "^16.5.0",
        "graphql-request": "^4.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-imgix": "^9.5.2"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.16.0",
        "@remix-run/eslint-config": "^1.16.0",
        "@remix-run/serve": "^1.16.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/react-imgix": "^9.5.0",
        "eslint": "^8.20.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.1.7",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=14"
    }
}
